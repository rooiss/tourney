{
  "name": "tourney-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development ts-node-dev src/server.ts",
    "mockdata": "ts-node ./src/scripts/createMockData",
    "stop": "docker-compose -f ./docker/dev/docker-compose.yml down",
    "copy:types": "cp -rf ./src/types/* ./client/src/types/",
    "watch:types": "npm-watch copy:types",
    "start:prod": "NODE_ENV=production nodemon build/server.js",
    "build": "tsc && npm run copy:non-ts",
    "copy:non-ts": "copyfiles -u 1 -e \"**/*.tsx?\" \"./src/**/*\" ./build/"
  },
  "watch": {
    "copy:types": {
      "patterns": [
        "./src/types/*"
      ],
      "extensions": "ts",
      "quiet": false
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rooiss/tourney.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rooiss/tourney/issues"
  },
  "homepage": "https://github.com/rooiss/tourney#readme",
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/node": "^15.0.3",
    "@types/nodemailer": "^6.4.2",
    "@types/passport": "^1.0.6",
    "@types/passport-local": "^1.0.33",
    "@types/redis": "^2.8.28",
    "@types/uuid": "^8.3.0",
    "copyfiles": "^2.4.1",
    "npm-watch": "^0.10.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "connect-redis": "^5.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "googleapis": "^87.0.0",
    "nodemailer": "^6.6.2",
    "nodemon": "^2.0.12",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.6.0",
    "redis": "^3.1.2",
    "typeorm": "^0.2.32",
    "uuid": "^8.3.2"
  }
}
